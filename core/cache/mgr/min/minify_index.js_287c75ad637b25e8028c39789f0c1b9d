MODx.page.Workspace=function(config){config=config||{};Ext.applyIf(config,{components:[{xtype:'modx-panel-workspace'}],buttons:[{text:_('help_ex'),handler:MODx.loadHelpPane}]});MODx.page.Workspace.superclass.constructor.call(this,config);Ext.Ajax.timeout=0;};Ext.extend(MODx.page.Workspace,MODx.Component);Ext.reg('modx-page-workspace',MODx.page.Workspace);